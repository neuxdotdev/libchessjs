{
  "name": "libchessjs",
  "version": "1.0.1",
  "description": "Unofficial TypeScript Library for Chess.com API - modern, fast, and fully typed.",
  "keywords": [
    "chess",
    "api",
    "typescript",
    "library",
    "chess.com",
    "wrapper",
    "rest-api"
  ],
  "homepage": "https://github.com/neuxdotdev/libchessjs#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neuxdotdev/libchessjs.git"
  },
  "license": "MIT",
  "author": {
    "name": "neuxdotdev",
    "url": "https://github.com/neuxdotdev"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rimraf dist dist-cjs",
    "build": "npm run clean && tsc && npm run build:cjs",
    "build:cjs": "tsc --module commonjs --outDir dist-cjs && echo '{\"type\":\"commonjs\"}' > dist-cjs/package.json",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "release": "standard-version"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "vitest": "^3.2.4"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.8.1",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.4.5",
    "typedoc": "^0.28.14",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
