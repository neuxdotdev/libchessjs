{
  "name": "libchessdotcomjs",
  "version": "1.0.0",
  "description": "Unified API, SDK, and CLI toolkit connecting Chess.com and Lichess into one seamless chess development experience.",
  "keywords": [
    "chess",
    "api",
    "sdk",
    "cli",
    "typescript",
    "chess.com",
    "lichess",
    "chess-engine",
    "developer-tools"
  ],
  "homepage": "https://github.com/neuxdotdev/libchessdotcomjs#readme",
  "bugs": {
    "url": "https://github.com/neuxdotdev/libchessdotcomjs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neuxdotdev/libchessdotcomjs.git"
  },
  "license": "MIT",
  "author": {
    "name": "neuxdotdev",
    "email": "dev@neuxdot.dev",
    "url": "https://github.com/neuxdotdev"
  },
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli.js",
      "types": "./dist/cli.d.ts"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "openchess": "./dist/cli.js"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "rimraf dist && tsc --project tsconfig.json",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --max-warnings=0",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,json,md}\"",
    "type-check": "tsc --noEmit",
    "prepare": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "release": "npm publish --access public"
  },
  "dependencies": {
    "chess-web-api": "^1.2.0",
    "node-fetch": "^3.3.2",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^24.7.2",
    "@types/node-fetch": "^2.6.13",
    "cpx": "^1.5.0",
    "form-data": "^4.0.4",
    "rollup": "^4.52.4",
    "rollup-plugin-dts": "^6.2.3",
    "undici-types": "^7.16.0"
  }
}
